{
	"name": "mediawiki/whats-nearby",
	"type": "mediawiki-extension",
	"description": "Provides geolocation information to templates",
	"keywords": [
		"MediaWiki",
		"Geolocation",
		"Maps",
		"semantic queries",
		"semantic templates"
	],
	"homepage": "https://www.semantic-mediawiki.org/wiki/Extension:WhatsNearby",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "James Hong Kong",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "1.*,>=1.0.1",
		"mediawiki/maps": "~3.5|~4|~5|~6|~7",
		"onoi/shared-resources": "~0.1"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	},
	"autoload": {
		"files" : [
			"DefaultSettings.php",
			"WhatsNearby.php"
		],
		"psr-4": {
			"WNBY\\": "src/"
		}
	},
	"config": {
		"process-timeout": 0
	},
	"scripts":{
		"phpunit": "php ../../tests/phpunit/phpunit.php -c phpunit.xml.dist"
	}
}
